{
    "store.custom#institucionales": {
      "blocks": [
        "responsive-layout.desktop#inst",
        "responsive-layout.tablet#inst",
        "responsive-layout.phone#inst"
      ]
    },
    "responsive-layout.desktop#inst":{
        "children":[
            "flex-layout.row#imageCat",
            "flex-layout.row#textImg",
            "flex-layout.row#textInst"
        ]
    },
    "responsive-layout.tablet#inst":{
        "children":[
            "flex-layout.row#imageCat",
            "flex-layout.row#textImg",
            "flex-layout.row#textInst"
        ]
    },
    "responsive-layout.phone#inst":{
        "children":[
            "flex-layout.row#imageCatMob",
            "flex-layout.row#textImg",
            "flex-layout.row#textInst"
        ]
    },
    "flex-layout.row#imageCatMob":{
        "children":[
          "image#categoryImageMob"
        ],
        "props":{
          "blockClass": "imageCatMob",
          "fullWidth": true
        }
      },
      "image#categoryImageMob":{
        "props":{
          "src": "assets/infobanner4.png",
          "blockClass": "imgcat"
        }
      },
    "flex-layout.row#textImg":{
        "children":[
          "rich-text#imgText"
        ],
        "props":{
          "blockClass": "textImg",
          "title": "textPage"
        }
      },
      "rich-text#imgText":{
        "props":{
          "text": "AVISO DE PRIVACIDAD",
          "blockClass": "txtcat"
        }
      },
    "flex-layout.row#textInst": {
      "children": [
        "flex-layout.col#text-inst"
      ]
    },
  
    "flex-layout.col#text-inst": {
      "children": [
        "rich-text#inst"
      ],
      "props": {
        "blockclass": "textColumn",
        "preventVerticalStretch": true
      }
    },
  
    "rich-text#title-inst": {
      "props": {
        "text":
        "##Aviso de privacidad",
        "blockClass": "instTitle"
      }
    },
  
    "rich-text#inst": {
      "props": {
        "text": "Aqu√≠ va texto",
        "blockClass": "instText"
      }
    }
  }
  